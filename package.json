{
  "name": "@activeledger/sdk-bip39",
  "version": "1.0.3",
  "description": "NodeJS package for use when integrating with Activeledger with BIP39 Support",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config=jestconfig.json --detectOpenHandles",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "pretty": "npm run lint && npm run format",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push public && git push public --tags"
  },
  "homepage": "https://activeledger.io",
  "keywords": [
    "activeledger",
    "nodejs",
    "sdk",
    "blockchain",
    "dlt",
    "distributed ledger technology",
    "ledger",
    "smart contracts",
    "activeledger",
    "network",
    "node",
    "bip39"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeledger/SDK-NodeJS-BIP39.git"
  },
  "bugs": {
    "url": "https://github.com/activeledger/SDK-NodeJS-BIP39/issues"
  },
  "devDependencies": {
    "@types/eventsource": "1.1.15",
    "@types/jest": "29.5.9",
    "@types/node": "20.9.3",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@activeledger/sdk": "^1.3.6",
    "bip39": "3.1.0"
  }
}
